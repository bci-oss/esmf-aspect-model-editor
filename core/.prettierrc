{
  "printWidth": 140,
  "singleQuote": true,
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "bracketSpacing": false,
  "arrowParens": "avoid",
  "plugins": ["prettier-plugin-organize-attributes", "prettier-plugin-organize-imports"],
  "overrides": [
    {
      "files": ["*.yml", ".yaml"],
      "options": {
        "singleQuote": false
      }
    }
  ]
}
